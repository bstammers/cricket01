	<tr><td><b>Away Team</b></td></tr>
	<tr><td><%= @match.away_team %></td></tr>
	<tr><td><b>Start Date</b></td></tr>
	<tr><td><%= @match.start_date %></td></tr>
	<tr><td><b>End Date</b></td></tr>
