	<tr><td><b>Competition</b></td></tr>
	<tr><td><%= @match.competition %></td></tr>
	<tr><td><b>Grade</b></td></tr>
	<tr><td><%= @match.grade %></td></tr>
	<tr><td><b>Umpire 1</b></td></tr>
	<tr><td><%= @match.umpire1 %></td></tr>
	<tr><td><b>Umpire 2</b></td></tr>
